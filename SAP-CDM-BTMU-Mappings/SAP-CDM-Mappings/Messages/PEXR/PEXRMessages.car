		<validationrules name="CreditTransfer">
			<hdrvalidationrules>
				<fieldvalidations>
				</fieldvalidations>
			</hdrvalidationrules>
			<datavalidationrules>
				<fieldvalidations>
					<fieldvalidation>
						<name>E192</name>
						<fieldname>IDOC.Control.CREDAT</fieldname>
						<formula><![CDATA[$value.IsDate ("yyyyMMdd")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E192</error-code>
						<actionmessage><![CDATA["Should be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E193</name>
						<fieldname>IDOC.Control.CRETIM</fieldname>
						<formula><![CDATA[$value.IsDate ("HHmmss")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E193</error-code>
						<actionmessage><![CDATA["Should be a valid time of format 'HHmmss'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E196</name>
						<fieldname>IDOC.E2EDK03</fieldname>
						<formula><![CDATA[VerifyDate ($value[$value.$index], "028", true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E196</error-code>
						<actionmessage><![CDATA["When IDDAT is '028', DATUM should be a present and must be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E197</name>
						<fieldname>IDOC.E2IDPU1.E2EDP03</fieldname>
						<formula><![CDATA[VerifyDate ($value[$value.$index], "016", false)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E197</error-code>
						<actionmessage><![CDATA["When IDDAT is '016' and if DATUM is present, it should be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E198</name>
						<fieldname>IDOC.E2IDKU5.E2IDKU5.CUXKURS</fieldname>
						<formula><![CDATA[$value.IsDouble()]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E198</error-code>
						<actionmessage><![CDATA["Should be a valid numeric."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E199</name>
						<fieldname>IDOC.E2IDKU5.E2IDKU5.MOABETR</fieldname>
						<formula><![CDATA[$value.IsDouble()]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E199</error-code>
						<actionmessage><![CDATA["Should be a valid numeric."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E200</name>
						<fieldname>IDOC</fieldname>
						<comment><![CDATA[When both are present, either FIIBKCDE or FIIBKUCD should be a SWIFT code (equal to '002') in E2IDB02 with a given qualifier]]></comment>
						<formula><![CDATA[ValidateCodesInE2IDB02 ($value.E2IDB02, "BA")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E200</error-code>
						<actionmessage><![CDATA[GetValidateCodesInE2IDB02ErrMsg();]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E201</name>
						<fieldname>IDOC</fieldname>
						<comment><![CDATA[When both are present, either FIIBKCDE or FIIBKUCD should be a SWIFT code (equal to '002') in E2IDB02 with a given qualifier]]></comment>
