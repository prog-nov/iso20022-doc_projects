			</hdrvalidationrules>
			<datavalidationrules>
				<fieldvalidations>
					<fieldvalidation>
						<name>E255</name>
						<fieldname>IDOC.Control.CREDAT</fieldname>
						<formula><![CDATA[$value.IsDate ("yyyyMMdd")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E255</error-code>
						<actionmessage><![CDATA["Should be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E256</name>
						<fieldname>IDOC.Control.CRETIM</fieldname>
						<formula><![CDATA[$value.IsDate ("HHmmss")]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E256</error-code>
						<actionmessage><![CDATA["Should be a valid time of format 'HHmmss'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E259</name>
						<fieldname>IDOC.E2EDK03</fieldname>
						<formula><![CDATA[VerifyDate ($value[$value.$index], "028", true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E259</error-code>
						<actionmessage><![CDATA["When IDDAT is '028', DATUM should be a present and must be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E260</name>
						<fieldname>IDOC.E2IDPU1.E2EDP03</fieldname>
						<formula><![CDATA[VerifyDate ($value[$value.$index], "016", false)]]></formula>
						<severity>error</severity>
