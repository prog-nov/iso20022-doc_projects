						<cascade>true</cascade>
						<error-code>E193</error-code>
						<actionmessage><![CDATA["Should be a valid time of format 'HHmmss'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E196</name>
						<fieldname>IDOC.E2EDK03</fieldname>
						<formula><![CDATA[VerifyDate ($value[$value.$index], "028", true)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E196</error-code>
						<actionmessage><![CDATA["When IDDAT is '028', DATUM should be a present and must be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E197</name>
						<fieldname>IDOC.E2IDPU1.E2EDP03</fieldname>
						<formula><![CDATA[VerifyDate ($value[$value.$index], "016", false)]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E197</error-code>
						<actionmessage><![CDATA["When IDDAT is '016' and if DATUM is present, it should be a valid date of format 'yyyyMMdd'."]]></actionmessage>
					</fieldvalidation>
					<fieldvalidation>
						<name>E198</name>
						<fieldname>IDOC.E2IDKU5.E2IDKU5.CUXKURS</fieldname>
