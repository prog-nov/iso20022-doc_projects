	<folder name="Function Definitions" type="FunctionDefFolder">
		<FormulaFunctionDef name="ValidateRepeatingSection" type="FormulaFunctionDef">
			<description></description>
			<Category>User Defined</Category>
			<parameterfields>
				<parameterfield>
					<name>sec</name>
					<type>Section</type>
				</parameterfield>
				<parameterfield>
					<name>codeList</name>
					<type>StringList</type>
				</parameterfield>
				<parameterfield>
					<name>$codeField__TOKEN</name>
					<type>Token</type>
				</parameterfield>
				<parameterfield>
					<name>$valField__TOKEN</name>
					<type>Token</type>
				</parameterfield>
			</parameterfields>
			<ReturnType>Boolean</ReturnType>
			<Code><![CDATA[// Atmost one instance of the given section can have any of the given code values in the given code field
// If some val field is passed, then it must be present if the code field contains a value.

foreach (def val in codeList) {
	def count = 0;
	for (def i=0; i <sec.length(); i++) {
		if (sec[i].GetString ("$codeField__TOKEN") == val) 
			count = count + 1;
	}
	if (count != 0) {
		if ((count > 1) || ((Length ("$valField__TOKEN") != 0) && sec[0].IsNull ("$valField__TOKEN")))
			return false;
	}
}

return true;
]]></Code>
			<function-type>Formula</function-type>
		</FormulaFunctionDef>
