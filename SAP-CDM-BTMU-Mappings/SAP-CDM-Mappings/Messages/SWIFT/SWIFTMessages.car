<?xml version="1.0" encoding="UTF-8" ?>
<cartridge xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.0.2" name="SWIFTMessages" type="Cartridge">
	<version>1.0</version>
	<private>
		<summary>
			<service name="MT940_SRG2013" version="1.0" type="External Message">
			</service>
			<service name="MT942_SRG2013" version="1.0" type="External Message">
			</service>
		</summary>
