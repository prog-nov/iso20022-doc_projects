		<folder name="Function Definitions" type="FunctionDefFolder">
			<FormulaFunctionDef name="removePadChar" type="FormulaFunctionDef">
				<description></description>
				<Category>User Defined</Category>
				<parameterfields>
					<parameterfield>
						<name>str</name>
						<type>String</type>
					</parameterfield>
					<parameterfield>
						<name>padChar</name>
						<type>String</type>
					</parameterfield>
					<parameterfield>
						<name>justification</name>
						<type>String</type>
					</parameterfield>
				</parameterfields>
				<ReturnType>String</ReturnType>
				<Code><![CDATA[def startPos = 0;
def endPos = Length (str);
def increment = 1;

if (justification == "left") {
	startPos = Length (str) - 1;
	endPos = -1;
	increment = -1;
}

def ch = padChar.charAt (0);
def allPadChar = false;
while (!allPadChar && (str.charAt(startPos) == ch)) {
	startPos = startPos + increment;
	allPadChar = (startPos == endPos);
}

if (allPadChar)
	return "";
else if (startPos < endPos)
	return str.substring (startPos, endPos-startPos);

return str.substring (0, startPos-endPos);
]]></Code>
				<function-type>Formula</function-type>
			</FormulaFunctionDef>
			<elementproperties>
				<propertymap>
